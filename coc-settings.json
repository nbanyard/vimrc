{
  "codeLens.enable": true,
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/17.0.3/clangd_17.0.3/bin/clangd",
  "coc.preferences.formatOnSave": true,
  "languageserver": {
    "sql": {
      "filetypes": [
        "xsql"
      ],
      "module": "/usr/local/Cellar/sql-language-server/1.5.1/libexec/lib/node_modules/sql-language-server/dist/bin/cli.js",
      "args": [
        "up",
        "--method",
        "node-ipc"
